// Online C++ compiler to run C++ program online
#include <iostream>
#include <bits/stdc++.h>

int main() {
    /*
        c++ unordered map.
        take O(1)time to insert.
        so, total time is O(n) size of string.
        space O(k) less than size of string.
        
        // best algo.
    
    */
    std::string str="taat";
    std::unordered_map<char,int> count;
    
    for(int i=0;i<str.length();i++){
        count[str[i]]++;   
    }
    for(auto it : count){
        std::cout << it.first;
        std::cout << it.second;
    }
    return 0;
}
